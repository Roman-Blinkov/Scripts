class CartCounts
{
	public int EbooksCartSize { get; set; }
	public int LogosCartSize { get; set; }
	public int StoreCartSize { get; set; }
	public int UnknownCartSize { get; set; }
	
	public void Increment(string url)
	{
		if (url.StartsWith("https://ebooks.faithlife.com/"))
			++EbooksCartSize;
		else if (url.StartsWith("https://logos.com/"))
			++LogosCartSize;
		else if (url.StartsWith("https://store.faithlife.com/"))
			++StoreCartSize;
		else
			++UnknownCartSize;
	}
	
	public bool NeedsCheckout() => EbooksCartSize == 100 || LogosCartSize == 100 || StoreCartSize == 100;
	public void Reset() => EbooksCartSize = LogosCartSize = StoreCartSize = 0;
}

